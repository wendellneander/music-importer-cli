{
  "name": "music-importer-cli",
  "version": "1.0.0",
  "description": "A cli to import music from youtube or spotify",
  "author": "Wendell Neander",
  "main": "main.js",
  "bin": "main.js",
  "type": "module",
  "pkg": {
    "scripts": "src/**/*.js",
    "targets": ["node18-linux-x64", "node18-macos-x64", "node18-win-x64"],
    "outputPath": "dist"
  },
  "scripts": {
    "start": "node main.js"
  },
  "dependencies": {
    "axios": "^1.3.2",
    "dotenv": "^16.0.3",
    "ffmetadata": "^1.7.0",
    "ffmpeg": "^0.0.4",
    "fluent-ffmpeg": "^2.1.2",
    "inquirer": "^9.1.4",
    "superagent": "^8.0.3",
    "ytdl-core": "^4.11.2"
  }
}
